
config dnsproxy 'dnsproxy'
	option enabled '1'
	option fastest '1'
	option cache '1'
	option oversea_port '6051'
	option oversea_noipv6 '1'
	option mainland_port '6050'
	list oversea_dns 'tls://8.8.4.4'
	list oversea_dns 'tls://1.1.1.1'
	list mainland_dns '119.29.29.29'
	list mainland_dns '223.5.5.5'
	list mainland_dns '180.76.76.76'
